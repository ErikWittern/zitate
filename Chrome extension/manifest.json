{
	"manifest_version": 2,
	"name": "Zitate",
	"description": "This extension allows to extract citations from a Web page.",
	"version": "0.1",
	"minimum_chrome_version": "16.0.884",
	"permissions": ["<all_urls>"],
	"browser_action": {
		"default_icon": "zitate_icon.png",
		"default_popup": "popup.html"
	}
}